h Clean sunt testate dermatologic și aprobate de dermatologii Alianței pentru Sănătatea Pielii. Deoarece șervețelele umede Pampers Fresh Clean sunt concepute ținând cont de pielea delicată a bebelușilor, acestea nu conțin alcool, coloranți.</p>","children":[]},{"name":"Sfat depozitare (Conditii, Grade)","value":"A se păstra la loc uscat.","children":[]}]},{"type":"table","title":"Provenienta","subtitle":null,"attributes":[{"name":"Tara de origine","value":"Germania","children":[]}]}]}],"slots":[{"type":"products_carousel","title":"Produse similare","products":[{"code":"100092545","variantCode":"100092545","name":"Șervețele umede pentru bebeluși Sensitive Duo 2x52 buc","slug":"100092545-pampers-servetele-umede-pentru-bebelusi-sensitive-duo-2x52-buc","unitPriceLabel":"","brand":"Pampers","brandCode":"680","brandFilterCode":"pampers-b681","sku":"100092545","price":13.38,"originalPrice":null,"currencyCode":"RON","currency":"Lei","image":{"thumbnail":{"default":"https://cdn.freshful.ro/media/cache/sylius_shop_product_thumbnail/45/f0/cc912bef588b38c8a91dfe82fe84.jpg"},"large":{"default":"https://cdn.freshful.ro/media/cache/freshful_large/45/f0/cc912bef588b38c8a91dfe82fe84.jpg"},"extralarge":{"default":"https://cdn.freshful.ro/media/cache/sylius_shop_product_original/45/f0/cc912bef588b38c8a91dfe82fe84.jpg"}},"label":null,"tags":[],"isAvailable":true,"maxAvailableQuantity":100,"maxAllowedQuantity":100,"breadcrumbs":[{"code":"12","name":"Mama & copilul","slug":"12-mama-si-copilul"},{"code":"1202","name":"Igienă bebe","slug":"1202-igiena-bebe"},{"code":"120202","name":"Șervețele umede","slug":"120202-servetele-umede"},{"code":"100092545","name":"Șervețele umede pentru bebeluși Sensitive Duo 2x52 buc","slug":"100092545-pampers-servetele-umede-pentru-bebelusi-sensitive-duo-2x52-buc"}],"activePromotion":null,"saveMe":null,"labels":[],"geniusPrice":null,"geniusUnitPricePerLabel":null,"banner":null,"discountLabel":null,"geniusDiscountLabel":null,"badge":null},{"code":"100092538","