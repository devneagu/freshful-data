le.","children":[]}]},{"type":"table","title":"Provenienta","subtitle":null,"attributes":[{"name":"Nume producator","value":"KOLIOS S. A.","children":[]},{"name":"Website producator","value":"https://www.masline-grecia.ro","children":[]},{"name":"Origine","value":"Grecia","children":[]}]}]},{"title":"Ingrediente","attributeTypes":[{"type":"text","title":null,"subtitle":null,"attributes":[{"name":"Ingrediente","value":"Lapte de vacă degresat pasteurizat, lapte de vacă degresat condensat, smântână, proteine din lapte, culturi lactice.","children":[]},{"name":"Alergeni","value":"lapte și produse derivate (inclusiv lactoză)","children":[]}]},{"type":"nestedTable","title":null,"subtitle":"Continut caloric pentru 100 g","attributes":[{"name":"Energie (kcal)","value":"132","children":[]},{"name":"Energie (kj)","value":"548","children":[]},{"name":"Grasimi (grame)","value":"10","children":[{"name":" din care saturate/ acizi grasi saturati","value":"7","children":[]}]},{"name":"Carbohidrati/ Glucide (grame)","value":"4.5","children":[{"name":" din care zaharuri","value":"4","children":[]}]},{"name":"Proteine","value":"6","children":[]},{"name":"Sare / Sodium","value":"0.28","children":[]}]}]}],"slots":[{"type":"products_carousel","title":"Produse similare","products":[{"code":"100143881","variantCode":"100143881","name":"Iaurt grecesc 10% grăsime, 150g","slug":"100143881-kolios-iaurt-grecesc-10-grasime-150g","unitPriceLabel":"33,27 Lei/kg","brand":"Kolios","brandCode":"2781","brandFilterCode":"kolios-b2855","sku":"100143881","price":4.99,"originalPrice":null,"currencyCode":"RON","currency":"Lei","image":{"thumbnail":{"default":"https://cdn.freshful.ro/media/cache/sylius_shop_product_thumbnail/f8/7f/8212123b0e56e4f0e308f40ce6e7.jpg"},"large":{"default":"https://cdn.freshful.ro/media/cache/freshful_large/f8/7f/8212123b0e56e4f0e308f40ce6e7.jpg"},"extralarge":{"default":"https://cdn.freshful.ro/media/cache/sylius_shop_product_original/f8/7f/8212123b0e56e4f0e308f40ce6e7.jpg"}},"l